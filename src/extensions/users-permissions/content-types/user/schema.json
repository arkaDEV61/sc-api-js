{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "name": {
      "type": "string"
    },
    "surname": {
      "type": "string"
    },
    "year": {
      "type": "integer"
    },
    "phone": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "gender": {
      "type": "string"
    },
    "room_checkouts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::room-checkout.room-checkout",
      "mappedBy": "student"
    },
    "room": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::room.room",
      "inversedBy": "students"
    },
    "meal_checkouts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::meal-checkout.meal-checkout",
      "mappedBy": "student"
    },
    "faculty": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::faculty.faculty",
      "inversedBy": "students"
    },
    "academic_year": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::academic-year.academic-year",
      "inversedBy": "students"
    }
  }
}
